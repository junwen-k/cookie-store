{
  "name": "@cookie-store/solid",
  "description": "Reactive Cookie Store API binding for Solid.",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "cookie",
    "cookie-store",
    "solid",
    "solidjs",
    "solidstart",
    "signals",
    "reactive",
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "turbo watch build",
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@cookie-store/core": "workspace:*"
  },
  "devDependencies": {
    "@cookie-store/typescript-config": "workspace:*",
    "@solidjs/testing-library": "^0.8.10",
    "@types/node": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "tsup": "catalog:",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "solid-js": "^1.9.7"
  }
}
