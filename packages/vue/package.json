    "@vitejs/plugin-vue": "6.0.0",
    "tsup": "catalog:",
    "vitest": "catalog:",
    "vitest-browser-vue": "^1.0.0"
  },
  "peerDependencies": {
    "vue": "^3.5.17"
  }
