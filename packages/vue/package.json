{
  "name": "@cookie-store/vue",
  "description": "Vue composables for reactive Cookie Store API bindings",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "cookie",
    "cookie-store",
    "vue",
    "vue3",
    "composables",
    "reactive",
    "typescript"
