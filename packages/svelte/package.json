{
  "name": "@cookie-store/svelte",
  "description": "Svelte stores for reactive Cookie Store API bindings",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "cookie",
    "cookie-store",
    "svelte",
    "svelte5",
    "sveltekit",
    "reactive",
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "turbo watch build",
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@cookie-store/core": "workspace:*"
  },
  "devDependencies": {
    "@cookie-store/typescript-config": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@types/node": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "svelte": "^5.36.8",
    "svelte-check": "^4.2.2",
    "tsup": "catalog:",
    "vitest": "catalog:",
    "vitest-browser-svelte": "^0.1.0"
  },
  "peerDependencies": {
    "svelte": "^5.7.0"
  }
}
