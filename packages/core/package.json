{
  "name": "@cookie-store/core",
  "description": "Core synchronous cache for Cookie Store API - enables reactive bindings across frameworks.",
  "version": "0.1.0",
  "type": "module",
  "keywords": [
    "cookie",
    "cookie-store",
    "cache",
    "reactive",
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
